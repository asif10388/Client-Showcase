  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "concat": "^1.0.3",
    "node-sass": "^4.13.1"
  },
  "dependencies": {
    "postcss-cli": "^5.0.0"
  }
}
